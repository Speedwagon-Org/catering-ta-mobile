<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Redirector"
    android:orientation="vertical"
    android:gravity="center">


    <Button
        android:id="@+id/btn_redirect_auth"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Authentication"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/btn_redirect_otp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="OTP"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/btn_redirect_home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go To Home"
        tools:ignore="HardcodedText" />
    <Button
        android:id="@+id/btn_redirect_vendor_detail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go To Vendor Detail"
        tools:ignore="HardcodedText" />
    <Button
        android:id="@+id/btn_redirect_order_detail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go To Order Detail"
        tools:ignore="HardcodedText" />
</LinearLayout>